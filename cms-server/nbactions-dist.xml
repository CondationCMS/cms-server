<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.1.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-DCMS_HOME=../test-server</exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs>server start</exec.appArgs>
                <exec.mainClass>com.condation.cms.cli.CMSCli</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.workingdir>../test-server</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.1.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-DCMS_HOME=../test-server -agentlib:jdwp=transport=dt_socket,server=n,address=${jpda.address}</exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.appArgs>server start</exec.appArgs>
                <exec.mainClass>com.condation.cms.cli.CMSCli</exec.mainClass>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
                <exec.workingdir>../test-server</exec.workingdir>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:3.1.0:exec</goal>
            </goals>
            <properties>
                <exec.vmArgs>-DCMS_HOME=../test-server</exec.vmArgs>
                <exec.args>${exec.vmArgs} -classpath %classpath ${exec.mainClass} ${exec.appArgs}</exec.args>
                <exec.mainClass>com.condation.cms.cli.CMSCli</exec.mainClass>
                <exec.executable>java</exec.executable>
                <exec.appArgs>server start</exec.appArgs>
                <exec.workingdir>../test-server</exec.workingdir>
            </properties>
        </action>
    </actions>
